  }

  mountCallbacks(e, r) {
    this.storage[e].callbacks.onStub = (...s) => {
      r.onStub(...s), this.storage[e].callbacks.onStub = t;
    }, this.storage[e].callbacks.onError = (...s) => {
      r.onError(...s), this.storage[e].callbacks.onError = t;
    }, this.storage[e].callbacks.onRender = (...s) => {
      r.onRender(...s), this.storage[e].callbacks.onRender = t;
    };
  }

  enhanceParams(t, r) {
    return Object.assign(Object.assign({}, r), {
      params: Object.assign(Object.assign({}, r.params), r.cspNonce ? {
        eid1: r.cspNonce
      } : {}),
      onError: r => {
        const s = e();
        this.setError(t, r, s), this.storage[t] && this.storage[t].callbacks.onError(r, s);
      },
      onRender: () => {
        const r = e();
        this.setRendered(t, r), this.storage[t] && this.storage[t].callbacks.onRender(r);
      },
      onStub: () => {
        const r = e();
        this.setStub(t, r), this.storage[t] && this.storage[t].callbacks.onStub(r);
      }
    });
  }

  setMounted(t, r) {
    this.storage[t] && (this.storage[t].lifecycle = Object.assign(Object.assign({}, r), {
      timemark: e()
    }));
  }

  setError(e, t, r) {
    this.storage[e] && (this.storage[e].lifecycle = {
      stage: "finished",
      status: "error",
      error: t,
      timemark: r
    });
  }

  setRendered(e, t) {
    this.storage[e] && (this.storage[e].lifecycle = {
      stage: "finished",
      status: "rendered",
      timemark: t
    });
  }

  setStub(e, t) {
    this.storage[e] && (this.storage[e].lifecycle = {
      stage: "finished",
      status: "stub",
      timemark: t
    });
  }

  createRecord(e, r) {
    const {
      onStub: s = t,
      onRender: n = t,
      onError: o = t
    } = r,
          a = function (e, t) {
      var r = {};

      for (var s in e) Object.prototype.hasOwnProperty.call(e, s) && t.indexOf(s) < 0 && (r[s] = e[s]);

      if (null != e && "function" == typeof Object.getOwnPropertySymbols) {
        var n = 0;

        for (s = Object.getOwnPropertySymbols(e); n < s.length; n++) t.indexOf(s[n]) < 0 && Object.prototype.propertyIsEnumerable.call(e, s[n]) && (r[s[n]] = e[s[n]]);
      }

      return r;
    }(r, ["onStub", "onRender", "onError"]);

    this.storage[e] = {
      callbacks: {
        onStub: s,
        onRender: n,
        onError: o
      },
      params: a,
      lifecycle: {
        stage: "in-progress",
        timemark: void 0
      }
    };
  }

}();</script><div id="__next" data-reactroot=""><div class="styles_root__nRLZC" data-tid="a9c24010"><div class="styles_root__BJH2_ styles_headerContainer__f7XqC" data-tid="1d1e4a8c"><header class="styles_root__jlo2L styles_header__0LyFn styles_rootDark__nQDyF" data-tid="63b05890"><div class="header-navigation styles_navigation__S8raz styles_navigationDark__x0vdW styles_hasSidebar__rU_E2"><div class="styles_logoContainer__G47EP"><div class="styles_root__jfc_s" data-tid="911e2f4d"><div class="styles_navigationWithSidebar__ngKxF"><button class="styles_root__coHaQ styles_burger__HcbjK" type="button" aria-label="Меню" aria-haspopup="true" aria-expanded="false" data-tid="8ed90190"><svg width="24" height="24" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M21 7.102H3v-2.4h18v2.4Zm0 6H3v-2.4h18v2.4Zm0 6H3v-2.4h18v2.4Z" fill="#999999"></path></svg></button><div class="styles_dropdown__MqT__ styles_dropdownDefault__KwZHT" data-tid="585eb7c0"><div class="styles_dropdownMenu__bouwC styles_dropdownMenu__c7FZ4 styles_dropdownMenuDark__NzGxR"><nav class="styles_navigationMenu__c_jLJ styles_root__yc0fe" data-tid="e500879d"><ul><li data-tid="f76ee5ab"><a href="/" class="styles_root__7mPJN styles_currentPageLink__4UpKk styles_darkThemeItem__E_aGY" data-tid="de7c6530"><img loading="lazy" aria-hidden="true" class="styles_icon__PXEHs" src="https://avatars.mds.yandex.net/get-bunker/120922/4a5dd24b637255a8fc5190bb353ef60c21018288/orig" data-tid="b35288c1">Главная</a></li><li data-tid="f76ee5ab"><a href="https://hd.kinopoisk.ru/" class="styles_root__7mPJN styles_darkThemeItem__E_aGY" data-tid="de7c6530"><img loading="lazy" aria-hidden="true" class="styles_icon__PXEHs" src="https://avatars.mds.yandex.net/get-bunker/994123/9589a3fd442af2ca26c14e6d6e539d30399eb29c/orig" data-tid="b35288c1">Онлайн-кинотеатр</a></li><li data-tid="f76ee5ab"><a href="/lists/categories/movies/1/" class="styles_root__7mPJN styles_darkThemeItem__E_aGY" data-tid="de7c6530"><img loading="l