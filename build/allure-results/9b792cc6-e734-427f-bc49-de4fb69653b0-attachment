        <script nonce="">
        $(() => {
            let internalPriceElement = $('.input input[name="sum"]');
            let resultSumElement = $('.to-paid__price-val');
            let voucherCodeElement = $('.promoInput input[name="voucher"]');
            let voucherButtonElement = $('.promoInput input[type="submit"]');

            $(voucherCodeElement).on('input', () => {
                if (voucherButtonElement.is(':hidden')) {
                    voucherButtonElement.prop('hidden', false);
                }
            });

            resultSumElement.text(internalPriceElement.val() || 0);

            $(internalPriceElement).on('input', (e) => {

                setTimeout(() => {
                    let resultSum = $(e.target).val();
                    resultSum = (!resultSum || resultSum < 0) ? 0 : resultSum;

                    resultSumElement.text(resultSum);
                }, 300)
            });

            $('#clickCreateInternalBill').on('click', function () {
                let form = $(this).closest('form');

                if (form.hasClass('disabled')) {
                    return false;
                }

                form.addClass('disabled');
                $.ajax({
                    url: form.attr('data-action'),
                    method: 'POST',
                    data: form.serialize()
                }).done(function (data) {
                    if (data.isQr) {
                        $('#qrCode').html(data.qrPartial);
                        initWaitingForPayment();
                    } else if (data.success) {
                        window.location.href = data.success;
                    }
                    if (data.errors) {
                        showCustomErrors(form, data.errors);
                        showBorderErrors(form, data.errors);
                    }
                    form.removeClass('disabled');
                });
                return false;
            });

            $('#clickCreateInternalBillCert').on('click', function () {
                let form = $(this).closest('form');

                if (form.hasClass('disabled')) {
                    return false;
                }

                form.addClass('disabled');
                $.ajax({
                    url: form.attr('data-action'),
                    method: 'POST',
                    data: form.serialize()
                }).done(function (data) {
                    if (data.status === true) {
                        let modal = $('[data-modal-name="success-other"]');
                        modal.find('.modal-title').text(data.message);
                        modal.addClass('active');

                        $('.popupRight#replenishmentBalance').toggleClass('active');
                        $('body').toggleClass('open-balance');

                        // $('.balance__count').text(data.internalBill);
                        $(voucherCodeElement).val('');
                        $('#card-tab').click();
                    }
                    if (data.errors) {
                        let modal = $('[data-modal-name="error-coupon"]');
                        modal.find('.modal-title').text(data.message);
                        modal.addClass('active');
                    }
                })
                .fail(function(data, qwe) {
                    if(data.status === 429) {
                        let modal = $('[data-modal-name="error-coupon"]');
                        modal.find('.modal-title').text('Повторите попытку позднее');
                        modal.addClass('active');
                    }
                })
                .always(function() {
                    form.removeClass('disabled');
                });
                return false;
            })
        })
    </script>
    <script nonce="">
        var searchSubsTime;
        $(document).ready(function() {
            // $('#mySubscribers').on('shown.bs.modal', function () {
            //     $('#mySubscribers').find('.my-subscription-search .close').trigger('click');
            // })
            // $('#mySubscriptions').on('shown.bs.modal', function () {
            //     $('#mySubscriptions').find('.my-subscription-search .close').trigger('click');
            // })
            $('.aboutMe .edit').on('click', function() {
                var $this = $(this);
                var $textarea = $this.closest('.aboutMe').find('textarea');
                var $text = $this.closest('.aboutMe').find('p.text');
                var $showBtn = $this.closest('.aboutMe').find('.show');
                if ($this.data('state') === '0') {
                    $.ajax({
                        url: 'https://shop.tastycoffee.ru/profile/update-about',
                        method: 'POST',
                        data: {about: $textarea.val()},
                    }).done(function () {
                        $('.text-full').remove();
                        $text.text($textarea.val() || 'Не заполнено');
                        $textarea.hide();
                        $text.show();
                        $this.text('Редактировать');
                        $this.data('state', '1');
                        $showBtn.hide();
                    }).fail(function (data) {
                        if (data.status === 422) {
                            alert(Object.values(data.responseJSON.errors).flat().join(', '));
                        }
                    });
                } else {
                    $textarea.show();
                    $text.hide();
                    $('.show-full').hide();
                    $this.text('Сохранить');
                    $this.data('state', '0');
                }
            });
            $('.my-subscription-modal').on('submit', function () {
                searchSubs(this);
                return false;
            });
            $('.my-subscription-modal').on('input', '[name="subs_q"]', function () {
                searchSubs($(this).closest('form'));
                return false;
            });
        });

        $(window).on('load', function () {
            var $target = null;
            
            scrollToElement($target);
        });

        function searchSubs(form) {
            clearTimeout(searchSubsTime);
            searchSubsTime = setTimeout(function () {
                var $form = $(form);
                var formData = new FormData($form[0]);

                $.ajax({
                    url: 'https://shop.tastycoffee.ru/ajax/social/search/subs',
                    method: 'POST',
                    data: formData,
                    cache: false,
                    contentType: false,
                    processData: false,
                }).done(function (data) {
                    $form.closest('.modal-content').find('.my-subscription-list').html(data.partial || '');
                });
            }, 500);
        }

        function openSubsModal(el, modalId) {
            var $elem = $(el);
            if ($elem.hasClass('disable')) {
                return false;
            }

            $elem.addClass('disable');
            $.ajax({
                url: 'https://shop.tastycoffee.ru/ajax/social/subs_modal',
                method: 'POST',
                data: {
                    q_type: modalId === 'mySubscribers' ? 'subscribers' : 'subscriptions',
                    user_id: '314318',
                    subs_q: '',
                }
            }).done(function (data) {
                if (data.partial) {
                    $('#' + modalId).find('.modal-content').html(data.partial);
                }
                $('#' + modalId).modal('show');
                $elem.removeClass('disable');
            });
            return false;
        }

        $('.show-full').on('click', function () {
            var $textFull = $('.text-full');
            var $textShort = $('.text.short');
            if ($(this).data('open') === true) {
                $(this).data('open', false);
                $textFull.hide();
                $textShort.show();
                $(this).text('Показать полностью');
            } else {
                $(this).data('open', true);
                $textFull.show();
                $textShort.hide();
                $(this).text('Скрыть');
            }
        });

        $('#update-filters').on('click', function () {
            var $elem = $(this);
            if ($elem.hasClass('disable')) {
                return false;
            }

            $elem.addClass('disable');

            var checked = [];
            $('[name="filters[]"]:checked').each(function () {
                checked.push($(this).val());
            });

            var other_filter = '';
            if($('#how-cook-other').is(":checked")) {
                other_filter = $('[name="filters_other_text"]').val();
            }

            $.ajax({
                url: 'https://shop.tastycoffee.ru/profile/update-filters',
                data: {
                    filters: checked,
                    other_filter: other_filter,
                },
                method: 'POST',
            }).done(function (data) {

                $('.filters').html(data);
                $('#how_cook').modal('hide');
            }).always(function ()  {
                $elem.removeClass('disable');
            }).fail(function (data) {
                alert(data.responseJSON.message || 'неизвестная ошибка');
            });
        });

        // let portion = $('.portion')
        // for(let portions of portion){
        //     let dataPercent = portions.attr('dataPercent')
        //     console.log(dataPercent + "SSSS")
        // }
        $('.portion').each(function() {
            var percentCount = $(this).attr('data-percent');
            var circleL = $(this).find('.circle-l span')
            var circleR = $(this).find('.circle-r span')
            var cccx = 180 + percentCount * 3.6
            var cccy = percentCount * 3.6
            if(percentCount <= 50){
                circleL.css({'opacity' : '0'})
                circleR.css({'transform' : 'rotate(' + cccx + 'deg)'})
            }else {
                circleR.css({'transform' : 'rotate(0deg)'})
                circleL.css({'transform' : 'rotate(' + cccy + 'deg)'})
            }
        });
    </script>
                            <script nonce="">
                              $(function () {
                                $("#openSearch_h").on("click", function () {
                                  $("#search_h").val("")
                                  $(".headerProduct-search").html("")
                                })

                              })
                            </script>
                            <script nonce="" type="text/javascript">

      function setCookie(name, val) {
        // Set a cookie named "cookieName" with a value of "cookieValue" and an expiration date of 7 days
        var expires = ""
        var date = new Date()
        date.setTime(date.getTime() + (7 * 24 * 60 * 60 * 1000))
        expires = "; expires=" + date.toUTCString()
        document.cookie = name + "=" + (val || "") + expires + "; path=/"
      }

      function removeCookie(name) {
        document.cookie = name + "=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;"
      }

      window.onload = function () {
        if ($("#yandex_oauth").length) {
          if (!document.cookie.includes("auth-popup-seen")) {
            if (document.cookie.includes("preffer_auth=yandex")) {
              yandexPopupInit()
            } else {
              initAuthPopups()
            }
          } else {
            console.log("skip auth popup")
          }

          function yandexPopupInit() {
            setCookie("auth-popup-seen", "1")

            if ($("#yandex_oauth").length) {
              const {client_id, base_url, redirect} = JSON.parse($("#yandex_oauth").val())

              console.log("yandex oauth init")
              YaAuthSuggest.init({
                  client_id: client_id,
                  response_type: "token",
                  redirect_uri: redirect,
                },
                base_url,
              ).then(({handler}) => handler()).then(data => {
                $.ajax({
                  url: "/ajax/social/yandex",
                  method: "POST",
                  data: data,
                }).done(function (data) {
                  console.log(data)
                  setCookie("preffer_auth", "yandex")
                  removeCookie("auth-popup-seen")
                  window.location.reload()
                })
                console.log("Сообщение с токеном", data)
              }).catch(error => console.log("Обработка ошибки", error))

            }
          }

          function initAuthPopups() {

            const {client_id} = JSON.parse($("#google_oauth").val())

            google.accounts.id.initialize({
              client_id: client_id,
              callback: handleCredentialResponse,
            })

            google.accounts.id.prompt((notification) => {
              console.log("google callback")

              setCookie("auth-popup-seen", "1")

              if (notification.isNotDisplayed()) {
                // try next provider if OneTap is not displayed or skipped
                yandexPopupInit()
              }
            })

            function handleCredentialResponse(response) {
              $.post("/ajax/auth/google/one-tap/callback", response, function (response) {
                // Success callback function
                console.log("Success:")
                setCookie("preffer_auth", "google")
                removeCookie("auth-popup-seen")

                window.location.reload()
              }).fail(function () {
                // Error callback function
                console.log("Error")
              })
              console.log(response)
            }
          }
        }

      }
    </script>
    <div id="user_auth"></div>

    <script nonce="" type="text/javascript" async="" defer="">(function () {
            var s = document.createElement('script');
            s.type = 'text/javascript';
            s.id = 'supportScript';
            s.charset = 'utf-8';
            s.async = true;
            s.defer = true;
            s.src = '//me-talk.ru/support/support.js?h=1645ced87d2f67dd699696c718a5dfdd';
            var sc = document.getElementsByTagName('script')[0];
            var callback = function () {/* Здесь вы можете вызывать API. Например, чтобы изменить отступ по высоте: supportAPI.setSupportTop(200); */
            };
            s.onreadystatechange = s.onload = function () {
                var state = s.readyState;
                if (!callback.done && (!state || /loaded|complete/.test(state))) {
                    callback.done = true;
                    callback();
                }
            };
            if (sc) sc.parentNode.insertBefore(s, sc); else document.documentElement.firstChild.appendChild(s);
        })();</script>


